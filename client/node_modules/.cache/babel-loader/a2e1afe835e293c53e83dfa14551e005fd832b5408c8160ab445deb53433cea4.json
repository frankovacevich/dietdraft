{"ast":null,"code":"import TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport { mainStore } from \"@/store\";\nexport default {\n  __name: 'AddFoodModal',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = mainStore();\n    const __returned__ = {\n      store,\n      TitleBar,\n      MainContainer,\n      get mainStore() {\n        return mainStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["TitleBar","MainContainer","mainStore","store"],"sources":["/home/x/workspace/01-diet-draft/client/src/views/AddFoodModal.vue"],"sourcesContent":["<script setup>\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport { mainStore } from \"@/store\";\nconst store = mainStore();\n</script>\n\n<template>\n  <div class=\"modal\" :class=\"{ 'modal-hidden': !store.addFoodModalVisible }\">\n    <TitleBar>\n      <div class=\"title-bar-icon\" @click=\"store.addFoodModalVisible = false\">\n        <font-awesome-icon icon=\"fa-solid fa-arrow-left\" />\n      </div>\n      <div style=\"flex-grow: 1\"></div>\n      <div>Add to {{ store.Meals[store.addFoodModalMeal] }}</div>\n      <div style=\"flex-grow: 1\"></div>\n      <div class=\"title-bar-icon\">\n        <font-awesome-icon icon=\"fa-solid fa-check\" />\n      </div>\n    </TitleBar>\n    <MainContainer> Hello </MainContainer>\n  </div>\n</template>\n\n<style scoped>\n.modal {\n  z-index: 9999;\n  position: fixed;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  transition: top 0.2s ease-in-out;\n}\n\n.modal-hidden {\n  top: 100vh;\n}\n</style>\n"],"mappings":"AACA,OAAOA,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,SAASC,SAAS,QAAQ,SAAS;;;;;;;IACnC,MAAMC,KAAK,GAAGD,SAAS,CAAC,CAAC"},"metadata":{},"sourceType":