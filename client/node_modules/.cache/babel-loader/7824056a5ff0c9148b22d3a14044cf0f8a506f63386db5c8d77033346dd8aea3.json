{"ast":null,"code":"import TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBars from \"@/components/TotalsBars.vue\";\nimport { mainStore } from \"@/store\";\nexport default {\n  __name: 'MainView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = mainStore();\n    const __returned__ = {\n      store,\n      TitleBar,\n      MainContainer,\n      FoodItem,\n      TotalsBars,\n      get mainStore() {\n        return mainStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["TitleBar","MainContainer","FoodItem","TotalsBars","mainStore","store"],"sources":["/home/x/workspace/01-diet-draft/client/src/views/MainView.vue"],"sourcesContent":["<script setup>\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBars from \"@/components/TotalsBars.vue\";\nimport { mainStore } from \"@/store\";\nconst store = mainStore();\n</script>\n\n<template>\n  <TitleBar>\n    <div>Day {{ store.day + 1 }}</div>\n    <div style=\"flex-grow: 1\"></div>\n    <div class=\"title-bar-icon\" @click=\"store.goToPreviousDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-left\" />\n    </div>\n    <div class=\"title-bar-icon\" @click=\"store.toggleEditMode\">\n      <font-awesome-icon\n        v-if=\"store.editMode\"\n        icon=\"fa-solid fa-pen-to-square\"\n      />\n      <font-awesome-icon\n        v-if=\"!store.editMode\"\n        icon=\"fa-regular fa-pen-to-square\"\n      />\n    </div>\n    <div class=\"title-bar-icon\" @click=\"store.goToNextDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-right\" />\n    </div>\n  </TitleBar>\n\n  <MainContainer>\n    <TotalsBars\n      :protein=\"store.todaysQuantities.protein\"\n      :fat=\"store.todaysQuantities.fat\"\n      :carbs=\"store.todaysQuantities.carbs\"\n      :proteinTarget=\"store.planInfo.protein\"\n      :fatTarget=\"store.planInfo.fat\"\n      :carbsTarget=\"store.planInfo.carbs\"\n    ></TotalsBars>\n\n    <div v-for=\"(food, index) in store.allFoods\" :key=\"index\">\n      <FoodItem\n        :name=\"food.name\"\n        :icon=\"food.icon\"\n        :description=\"food.description\"\n        :protein=\"food.protein\"\n        :fat=\"food.fat\"\n        :carbs=\"food.carbs\"\n        :amount=\"food.amount\"\n        :crossed=\"true\"\n      ></FoodItem>\n    </div>\n  </MainContainer>\n\n  <div\n    v-if=\"store.editMode\"\n    @click=\"store.recalculateToday()\"\n    class=\"round-button\"\n  >\n    <font-awesome-icon icon=\"fa-solid fa-rotate-right\" />\n  </div>\n</template>\n\n<style scoped>\n.round-button {\n  width: 56px;\n  height: 56px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: fixed;\n  bottom: 85px;\n  right: 15px;\n  background-color: var(--color-primary);\n  color: var(--color-text);\n  font-size: 16pt;\n  border-radius: 50%;\n}\n\n.round-button:active {\n  background-color: var(--color-primary-active);\n}\n</style>\n"],"mappings":"AACA,OAAOA,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,OAAOC,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,UAAU,MAAM,6BAA6B;AACpD,SAASC,SAAS,QAAQ,SAAS;;;;;;;IACnC,MAAMC,KAAK,GAAGD,SAAS,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}