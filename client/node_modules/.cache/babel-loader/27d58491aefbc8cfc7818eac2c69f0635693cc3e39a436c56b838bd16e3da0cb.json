{"ast":null,"code":"import draggable from \"vuedraggable\";\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBars from \"@/components/TotalsBars.vue\";\nimport AddFoodModal from \"./AddFoodModal.vue\";\nimport DropDownMenu from \"@/components/DropDownMenu.vue\";\nimport { Meals } from \"@/store\";\nimport { mainStore } from \"@/store\";\nexport default {\n  __name: 'MainView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = mainStore();\n    const __returned__ = {\n      store,\n      get draggable() {\n        return draggable;\n      },\n      TitleBar,\n      MainContainer,\n      FoodItem,\n      TotalsBars,\n      AddFoodModal,\n      DropDownMenu,\n      get Meals() {\n        return Meals;\n      },\n      get mainStore() {\n        return mainStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["draggable","TitleBar","MainContainer","FoodItem","TotalsBars","AddFoodModal","DropDownMenu","Meals","mainStore","store"],"sources":["/home/x/workspace/01-diet-draft/client/src/views/MainView.vue"],"sourcesContent":["<script setup>\nimport draggable from \"vuedraggable\";\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBars from \"@/components/TotalsBars.vue\";\nimport AddFoodModal from \"./AddFoodModal.vue\";\nimport DropDownMenu from \"@/components/DropDownMenu.vue\";\nimport { Meals } from \"@/store\";\nimport { mainStore } from \"@/store\";\nconst store = mainStore();\n</script>\n\n<template>\n  <AddFoodModal></AddFoodModal>\n  <TitleBar>\n    <div style=\"margin-left: 8px\">\n      Day {{ store.day + 1 }} / {{ store.planInfo.days }}\n    </div>\n    <div style=\"flex-grow: 1\"></div>\n    <div class=\"title-bar-icon\" @click=\"store.goToPreviousDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-left\" />\n    </div>\n    <div class=\"title-bar-icon\" @click=\"store.toggleEditMode\">\n      <font-awesome-icon\n        v-if=\"store.editMode\"\n        icon=\"fa-solid fa-pen-to-square\"\n      />\n      <font-awesome-icon\n        v-if=\"!store.editMode\"\n        icon=\"fa-regular fa-pen-to-square\"\n      />\n    </div>\n    <div class=\"title-bar-icon\" @click=\"store.goToNextDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-right\" />\n    </div>\n    <DropDownMenu>\n      <router-link to=\"/plan\">Plan</router-link>\n      <router-link to=\"/list\">Shopping list</router-link>\n    </DropDownMenu>\n  </TitleBar>\n\n  <MainContainer>\n    <TotalsBars\n      :protein=\"store.todaysQuantities.protein\"\n      :fat=\"store.todaysQuantities.fat\"\n      :carbs=\"store.todaysQuantities.carbs\"\n      :proteinTarget=\"store.planInfo.protein\"\n      :fatTarget=\"store.planInfo.fat\"\n      :carbsTarget=\"store.planInfo.carbs\"\n    ></TotalsBars>\n\n    <div v-for=\"(meal, i) in Meals\" :key=\"i\">\n      <div class=\"meal-title-container\">\n        <div class=\"meal-title\">{{ meal }}</div>\n        <div\n          v-if=\"store.editMode\"\n          class=\"meal-title-icon\"\n          @click=\"store.showAddFoodModal(i)\"\n        >\n          <font-awesome-icon icon=\"fa-solid fa-plus\" />\n        </div>\n        <div style=\"flex-grow: 1\"></div>\n        <div v-if=\"store.editMode\" class=\"meal-title-icon\">\n          <font-awesome-icon icon=\"fa-solid fa-trash\" />\n        </div>\n      </div>\n      <draggable\n        :list=\"store.todaysFoods[i]\"\n        item-key=\"id\"\n        tag=\"div\"\n        group=\"foods\"\n        :end=\"store.foodDragEnd()\"\n      >\n        <template #item=\"{ element: food }\">\n          <FoodItem\n            :name=\"food.name\"\n            :icon=\"food.icon\"\n            :description=\"food.description\"\n            :protein=\"food.protein\"\n            :fat=\"food.fat\"\n            :carbs=\"food.carbs\"\n            :amount=\"store.editMode || food.amount !== 1 ? food.amount : null\"\n            :crossed=\"food.selected\"\n            :selected-quantity=\"store.selectedQuantity\"\n            @bodyClick=\"if (store.editMode) store.changeFoodEaten(food);\"\n            @amountClick=\"if (store.editMode) store.changeFoodAmount(food);\"\n          ></FoodItem>\n        </template>\n      </draggable>\n    </div>\n  </MainContainer>\n\n  <div\n    v-if=\"store.editMode\"\n    class=\"round-button\"\n    @click=\"store.recalculateToday()\"\n  >\n    <font-awesome-icon icon=\"fa-solid fa-rotate-right\" />\n  </div>\n</template>\n\n<style scoped>\n.round-button {\n  width: 56px;\n  height: 56px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: fixed;\n  background-color: var(--color-primary);\n  color: var(--color-text);\n  font-size: 16pt;\n  border-radius: 50%;\n  bottom: 15px;\n  right: 15px;\n}\n\n.round-button:active {\n  background-color: var(--color-primary-active);\n}\n\n.meal-title-container {\n  display: flex;\n  align-items: center;\n}\n\n.meal-title-icon {\n  font-size: 12pt;\n  padding: 10px;\n}\n\n.meal-title-icon:active {\n  color: var(--color-primary-active);\n}\n\n.meal-title {\n  font-size: 12pt;\n  text-transform: capitalize;\n  color: var(--color-gray-1);\n  padding: 10px 0 10px 0;\n}\n</style>\n"],"mappings":"AACA,OAAOA,SAAS,MAAM,cAAc;AACpC,OAAOC,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,OAAOC,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,UAAU,MAAM,6BAA6B;AACpD,OAAOC,YAAY,MAAM,oBAAoB;AAC7C,OAAOC,YAAY,MAAM,+BAA+B;AACxD,SAASC,KAAK,QAAQ,SAAS;AAC/B,SAASC,SAAS,QAAQ,SAAS;;;;;;;IACnC,MAAMC,KAAK,GAAGD,SAAS,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}