{"ast":null,"code":"import TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport { mainStore } from \"@/store\";\nimport { Meals } from \"@/store\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nexport default {\n  __name: 'AddFoodModal',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = mainStore();\n    const __returned__ = {\n      store,\n      TitleBar,\n      MainContainer,\n      get mainStore() {\n        return mainStore;\n      },\n      get Meals() {\n        return Meals;\n      },\n      FoodItem\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["TitleBar","MainContainer","mainStore","Meals","FoodItem","store"],"sources":["/home/x/workspace/01-diet-draft/client/src/views/AddFoodModal.vue"],"sourcesContent":["<script setup>\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport { mainStore } from \"@/store\";\nimport { Meals } from \"@/store\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nconst store = mainStore();\n</script>\n\n<template>\n  <div class=\"modal\" :class=\"{ 'modal-hidden': !store.addFoodModalVisible }\">\n    <TitleBar>\n      <div class=\"title-bar-icon\" @click=\"store.addFoodModalVisible = false\">\n        <font-awesome-icon :icon=\"['fas', 'xmark']\" />\n      </div>\n      <div style=\"flex-grow: 1\"></div>\n      <div>Add {{ Meals[store.addFoodModalMeal] }}</div>\n      <div style=\"flex-grow: 1\"></div>\n      <div class=\"title-bar-icon\" @click=\"store.closeAddFoodModal()\">\n        <font-awesome-icon icon=\"fa-solid fa-check\" />\n      </div>\n    </TitleBar>\n    <MainContainer>\n      <FoodItem\n        v-for=\"(food, i) in store.addFoodModalFoods\"\n        :key=\"i\"\n        :name=\"food.name\"\n        :icon=\"food.icon\"\n        :description=\"food.description\"\n        :protein=\"food.protein\"\n        :fat=\"food.fat\"\n        :carbs=\"food.carbs\"\n        :amount=\"food.amount\"\n        :selected=\"food.selected\"\n        @bodyClick=\"food.selected = !food.selected\"\n      ></FoodItem>\n    </MainContainer>\n  </div>\n</template>\n\n<style scoped>\n.modal {\n  z-index: 9999;\n  position: fixed;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  transition: top 0.2s ease-in-out;\n}\n\n.modal-hidden {\n  top: 100vh;\n}\n</style>\n"],"mappings":"AACA,OAAOA,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,SAASC,SAAS,QAAQ,SAAS;AACnC,SAASC,KAAK,QAAQ,SAAS;AAC/B,OAAOC,QAAQ,MAAM,2BAA2B;;;;;;;IAChD,MAAMC,KAAK,GAAGH,SAAS,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}