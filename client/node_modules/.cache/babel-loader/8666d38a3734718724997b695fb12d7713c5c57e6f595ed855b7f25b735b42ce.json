{"ast":null,"code":"import draggable from \"vuedraggable\";\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBar from \"@/components/TotalsBar.vue\";\nimport AddFoodModal from \"./AddFoodModal.vue\";\nimport { mainStore } from \"@/store\";\nexport default {\n  __name: 'MainView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = mainStore();\n    const __returned__ = {\n      store,\n      get draggable() {\n        return draggable;\n      },\n      TitleBar,\n      MainContainer,\n      FoodItem,\n      TotalsBar,\n      AddFoodModal,\n      get mainStore() {\n        return mainStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["draggable","TitleBar","MainContainer","FoodItem","TotalsBar","AddFoodModal","mainStore","store"],"sources":["/home/x/workspace/diet-draft/client/src/views/MainView.vue"],"sourcesContent":["<script setup>\nimport draggable from \"vuedraggable\";\nimport TitleBar from \"@/components/TitleBar.vue\";\nimport MainContainer from \"@/components/MainContainer.vue\";\nimport FoodItem from \"@/components/FoodItem.vue\";\nimport TotalsBar from \"@/components/TotalsBar.vue\";\nimport AddFoodModal from \"./AddFoodModal.vue\";\nimport { mainStore } from \"@/store\";\nconst store = mainStore();\n</script>\n\n<template>\n  <AddFoodModal></AddFoodModal>\n  <TitleBar>\n    <div style=\"margin-left: 8px\">\n      Day {{ store.day + 1 }} / {{ store.planInfo.days }}\n    </div>\n    <div style=\"flex-grow: 1\"></div>\n    <div class=\"title-bar-icon\" @click=\"store.goToPreviousDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-left\" />\n    </div>\n    <router-link to=\"/plan\">\n      <div class=\"title-bar-icon\">\n        <font-awesome-icon icon=\"fa-solid fa-map\" />\n      </div>\n    </router-link>\n    <div class=\"title-bar-icon\" @click=\"store.toggleEditMode\">\n      <font-awesome-icon\n        v-if=\"store.editMode\"\n        icon=\"fa-solid fa-pen-to-square\"\n      />\n      <font-awesome-icon\n        v-if=\"!store.editMode\"\n        icon=\"fa-regular fa-pen-to-square\"\n      />\n    </div>\n    <div class=\"title-bar-icon\" @click=\"store.goToNextDay\">\n      <font-awesome-icon icon=\"fa-solid fa-chevron-right\" />\n    </div>\n  </TitleBar>\n\n  <MainContainer>\n    <div class=\"total-bars-container\">\n      <div @click=\"store.selectedQuantity = 1\">\n        <TotalsBar\n          :text=\"'Protein' + (store.selectedQuantity === 1 ? ' ~' : '')\"\n          :value=\"store.todaysQuantities.protein\"\n          :target=\"store.planInfo.protein\"\n        ></TotalsBar>\n      </div>\n      <div @click=\"store.selectedQuantity = 2\">\n        <TotalsBar\n          :text=\"'Fat' + (store.selectedQuantity === 2 ? ' ~' : '')\"\n          :value=\"store.todaysQuantities.fat\"\n          :target=\"store.planInfo.fat\"\n        ></TotalsBar>\n      </div>\n      <div @click=\"store.selectedQuantity = 3\">\n        <TotalsBar\n          :text=\"'Carbs' + (store.selectedQuantity === 3 ? ' ~' : '')\"\n          :value=\"store.todaysQuantities.carbs\"\n          :target=\"store.planInfo.carbs\"\n        ></TotalsBar>\n      </div>\n      <div @click=\"store.selectedQuantity = 0\">\n        <TotalsBar\n          :text=\"'Calories' + (store.selectedQuantity === 0 ? ' ~' : '')\"\n          :value=\"store.todaysQuantities.calories\"\n          :target=\"store.planInfo.calories\"\n        ></TotalsBar>\n      </div>\n    </div>\n\n    <div v-for=\"(meal, m) in store.meals\" :key=\"m\">\n      <div class=\"meal-title-container\">\n        <div class=\"meal-title\">{{ meal }}</div>\n        <div\n          v-if=\"store.editMode\"\n          class=\"meal-title-icon\"\n          @click=\"store.addFoodModal.open(m, store.allFoods)\"\n        >\n          <font-awesome-icon icon=\"fa-solid fa-plus\" />\n        </div>\n        <div style=\"flex-grow: 1\"></div>\n      </div>\n      <draggable\n        :list=\"store.todaysFoods[m]\"\n        item-key=\"id\"\n        tag=\"div\"\n        group=\"foods\"\n        :end=\"store.foodDragEnd()\"\n        :disabled=\"!store.editMode\"\n      >\n        <template #item=\"{ element: food, index: j }\">\n          <FoodItem\n            :name=\"food.name\"\n            :icon=\"food.icon\"\n            :description=\"food.description\"\n            :protein=\"food.protein\"\n            :fat=\"food.fat\"\n            :carbs=\"food.carbs\"\n            :amount=\"store.editMode || food.amount !== 1 ? food.amount : null\"\n            :crossed=\"food.selected\"\n            :selected-quantity=\"store.selectedQuantity\"\n            @bodyClick=\"store.changeFoodEaten(food)\"\n            @amountClick=\"if (store.editMode) store.changeFoodAmount(food);\"\n            @hold=\"store.removeFood(m, j)\"\n          ></FoodItem>\n        </template>\n      </draggable>\n    </div>\n  </MainContainer>\n\n  <div class=\"bottom-toolbar\" v-if=\"store.editMode\">\n    <div\n      class=\"round-button round-button-secondary\"\n      @click=\"store.clearToday()\"\n    >\n      <font-awesome-icon icon=\"fa-solid fa-trash\" />\n    </div>\n    <div class=\"round-button\" @click=\"store.recalculateToday()\">\n      <font-awesome-icon icon=\"fa-solid fa-rotate-right\" />\n    </div>\n  </div>\n</template>\n\n<style scoped>\n.bottom-toolbar {\n  position: fixed;\n  bottom: 15px;\n  left: 0;\n  right: 0;\n  height: 56px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  gap: 10px;\n}\n\n.round-button {\n  width: 56px;\n  height: 56px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background-color: var(--color-primary);\n  color: var(--color-text);\n  font-size: 16pt;\n  border-radius: 15px;\n  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1), 0 6px 20px rgba(0, 0, 0, 0.12);\n}\n\n.round-button:active {\n  background-color: var(--color-primary-active);\n}\n\n.round-button-secondary {\n  background-color: var(--color-gray-1);\n}\n\n.round-button-secondary:active {\n  background-color: var(--color-gray-2);\n}\n\n.meal-title-container {\n  display: flex;\n  align-items: center;\n}\n\n.meal-title-icon {\n  font-size: 12pt;\n  padding: 10px;\n}\n\n.meal-title-icon:active {\n  color: var(--color-primary-active);\n}\n\n.meal-title {\n  font-size: 12pt;\n  text-transform: capitalize;\n  color: var(--color-gray-1);\n  padding: 10px 0 10px 0;\n}\n\n.total-bars-container {\n  display: flex;\n  margin-top: 10px;\n  margin-bottom: 20px;\n}\n\n.total-bars-container div {\n  flex-grow: 1;\n}\n</style>\n"],"mappings":"AACA,OAAOA,SAAS,MAAM,cAAc;AACpC,OAAOC,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,OAAOC,QAAQ,MAAM,2BAA2B;AAChD,OAAOC,SAAS,MAAM,4BAA4B;AAClD,OAAOC,YAAY,MAAM,oBAAoB;AAC7C,SAASC,SAAS,QAAQ,SAAS;;;;;;;IACnC,MAAMC,KAAK,GAAGD,SAAS,CAAC,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}